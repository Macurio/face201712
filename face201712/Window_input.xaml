<Window x:Class="face201712.Window_input"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Window_input" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="text_uid" HorizontalAlignment="Left" Height="23" Margin="104,49,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged"/>
        <TextBox x:Name="text_gid" HorizontalAlignment="Left" Height="23" Margin="104,100,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="text_uifo" HorizontalAlignment="Left" Height="23" Margin="104,149,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button Content="确定" HorizontalAlignment="Left" Margin="104,195,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Label Content="uid" HorizontalAlignment="Left" Margin="42,47,0,0" VerticalAlignment="Top"/>
        <Label Content="uinfo" HorizontalAlignment="Left" Margin="42,147,0,0" VerticalAlignment="Top"/>
        <Label Content="gid" HorizontalAlignment="Left" Margin="42,98,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
